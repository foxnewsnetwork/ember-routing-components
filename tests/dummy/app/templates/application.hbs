{{#dummy/redux-anchor as |state actions|}}
  {{#routing/endpoint-base
    name=(route-for 'APP')
    state=state
    checkRoute=actions.checkRoute
    redirectRoute=actions.redirectRoute as |redux actions|}}
    {{#if redux.isActive}}
      {{partial 'routes/redux'}}

      {{redux.redirect to=(route-for 'HOME')}}
    {{/if}}
  {{/routing/endpoint-base}}
{{/dummy/redux-anchor}}