{{#with (routing/curry-fn checkActive routeId) as |nextCheckActive|}}
  {{#with (routing/curry-fn redirect routeId) as |nextRedirect|}}
    {{#routing/call-fn routeId nextCheckActive as |isActive|}}
      {{yield
        (hash
          isActive=isActive
          route=(component 'routing/-route' nextCheckActive nextRedirect)
          redirect=(component 'routing/-redirect' nextRedirect)
        )
        (hash
          redirect=nextRedirect
        )
      }}
    {{/routing/call-fn}}
  {{/with}}
{{/with}}